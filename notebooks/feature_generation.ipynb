    "def MaxWordLength(str): \n",
    "    strLen = len(str) \n",
    "    save = 0; currentLength = 0\n",
    "      \n",
    "    for i in range(0, strLen): \n",
    "        if (str[i] != ' '): \n",
    "            currentLength += 1\n",
    "        else: \n",
    "            save = max(save, currentLength) \n",
    "            currentLength = 0\n",
    "\n",
